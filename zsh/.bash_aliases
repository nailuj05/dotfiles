alias cd='z'
alias ls='eza --icons -H --group-directories-first --git -g'
alias tree='eza --icons -F -H --tree -r'
alias utopi='utop -init src/assignment.ml'
alias gcp='function _gcp() { git commit -am "$1" && git push; }; _gcp'
alias gcz='function _gcz() { git clone "$1" && cd "$(basename "$1" .git)" && nvim .; }; _gcz'
alias la='ls -a'
alias ll='ls -l'
alias ex='exit'
alias nv='nvim .'
alias pys='python ~/PyShell.py'
alias em='f() { emacsclient -cqn "${1}" & disown; }; f'
alias et='emacsclient -nw -cq'
alias ets='f() { emacsclient -nw -cq "/sudo::${1}"; }; f'
alias es='SUDO_EDITOR="emacsclient -t" sudoedit'
alias bc='bc -ql ~/.bcrc'
alias gtree='git log --oneline --graph --decorate --all'
alias remake='make clean && make'